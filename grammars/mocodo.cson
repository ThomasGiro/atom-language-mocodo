'scopeName': 'source.mocodo'
'name': 'Mocodo'
'fileTypes': [
	'mcd'
	'mocodo'
]

'patterns': [
	{
		'begin': '^'
		'end': ':'
		'patterns': [
			{
				'begin': '\\['
				'end': '\\]'
				'name': 'markup.italic.mocodo'
			}
			{
				'match': '(_11|..)[<>]?'
				'name': 'constant.numeric.mocodo'
			}
			{
				'match': '\\w'
				'name': 'constant.mocodo'
			}
		]
	}
]